    <string name="dummy_content">DUMMY\nCONTENT</string>
    <string name="fecha_de_nacimiento">Fecha de nacimiento</string>
    <string name="menu_gallery">Gallery</string>
    <string name="menu_home">Home</string>
    <string name="menu_slideshow">Slideshow</string>
    <string name="nav_header_desc">Navigation header</string>
    <string name="nav_header_subtitle">android.studio@android.com</string>
    <string name="nav_header_title">Android Studio</string>
    <string name="nombre_completo">Nombre completo</string>
    <string name="open">Open navigation drawer</string>
    <string name="registrar">Registrar</string>
    <string name="title_activity_home">HomeActivity</string>
    <string name="title_activity_menu">MenuActivity</string>
    <style name="Theme.PowerTechs" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        
        <item name="colorPrimary">@color/colorprimario</item>
        <item name="colorPrimaryVariant">@color/coloracento</item>
        <item name="colorOnPrimary">@color/white</item>
        
        <item name="colorSecondary">@color/colorfondo</item>
        <item name="colorSecondaryVariant">@color/colorbotones</item>
        <item name="colorOnSecondary">@color/black</item>
        
        <item name="android:statusBarColor">?attr/colorPrimaryVariant</item>
        
    </style>
    <style name="Theme.PowerTechs.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar"/>
    <style name="Theme.PowerTechs.Fullscreen" parent="Theme.PowerTechs">
        <item name="android:actionBarStyle">@style/Widget.Theme.PowerTechs.ActionBar.Fullscreen
        </item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="Theme.PowerTechs.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>
    <style name="Theme.PowerTechs.PopupOverlay" parent="ThemeOverlay.AppCompat.Light"/>
    <style name="ThemeOverlay.PowerTechs.FullscreenContainer" parent="">
        <item name="fullscreenBackgroundColor">@color/light_blue_600</item>
        <item name="fullscreenTextColor">@color/light_blue_A200</item>
    </style>
    <style name="Widget.Theme.PowerTechs.ActionBar.Fullscreen" parent="Widget.AppCompat.ActionBar">
        <item name="android:background">@color/black_overlay</item>
    </style>
    <style name="Widget.Theme.PowerTechs.ButtonBar.Fullscreen" parent="">
        <item name="android:background">@color/black_overlay</item>
        <item name="android:buttonBarStyle">?android:attr/buttonBarStyle</item>
    </style>
    <declare-styleable name="FullscreenAttrs">
        <attr format="color" name="fullscreenBackgroundColor"/>
        <attr format="color" name="fullscreenTextColor"/>
    </declare-styleable>
</resources>